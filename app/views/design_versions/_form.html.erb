<%= simple_form_for([@project, @design_version]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :project_id %>
    <%= f.input :html %>
    <!-- <%= f.input :comment_thread_count %> -->
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
