<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :label %>
    <%= f.input :description %>
    <%= f.input :artisan_id %>
    <%= f.input :design_versions_count %>
    <%= f.input :parent_project_id %>
    <%= f.input :show_in_catalog %>
    <%= f.input :product_category_id %>
    <%= f.input :customer_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
