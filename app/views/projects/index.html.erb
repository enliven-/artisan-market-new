
<div class="col-md-12">
  <div class="panel panel-default">
    <div class="panel-heading"><strong><span class="glyphicon glyphicon-th"></span>Listing Projects</strong></div>
    <div class="panel-body">
      <table class="table table-hover">
        <thead>
          <tr>
            <th>Label</th>
            <th>Description</th>
            <th>Artisan</th>
            <th>DesVers Count</th>
            <th>Parent Project</th>
            <th>In catalog</th>
            <th>Product Category</th>
            <th>Customer</th>
            <th></th>
            <th></th>
            <th></th>
          </tr>
        </thead>

        <tbody>
          <% @projects.each do |project| %>
          <tr>
            <td><%= project.label %></td>
            <td><%= project.description %></td>
            <td><%= project.artisan_id %></td>
            <td><%= project.design_versions_count %></td>
            <td><%= project.parent_project_id %></td>
            <td><%= project.show_in_catalog %></td>
            <td><%= project.product_category_id %></td>
            <td><%= project.customer_id %></td>
            <td><%= link_to 'Show', project %></td>
            <td><%= link_to 'Edit', edit_project_path(project) %></td>
            <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
          <% end %>
        </tbody>
      </table>

      <br>

      <%= link_to 'New Project', new_project_path %>
    
    </div>
  </div>
</div>
