<%= simple_form_for(@palette) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :label %>
    <%= f.input :user_id %>
    <%= f.input :project_id %>
    <%= f.input :product_category_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
